<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.materialdesign.materialdesignapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_demo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_demo_round"
        android:supportsRtl="true"
        android:theme="@style/AppMaterialTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActionBarToolbar" />
        <activity android:name=".StandAloneToolbar" />
        <activity android:name=".ContextualMenu"></activity>
    </application>

</manifest>